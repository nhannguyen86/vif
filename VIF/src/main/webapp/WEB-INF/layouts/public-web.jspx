<![CDATA[
<script type="text/javascript">
	var code = new Date().getTime() + '-' + '${login.id}';
	function resetCode() {
		code = new Date().getTime() + '-' + '${login.id}';
	}
	function getAppRootPath() {
		return '${home}';
	}
	function restrictInputDoubleOnly(event, sourceElement) {
		var value = sourceElement.value;
		var editedValue = "";
		for ( var i = 0; i < value.length; i++) {
			var currentChar = value[i];
			if (currentChar == '.') {
				if (editedValue.indexOf('.') < 0) {
					editedValue = editedValue + currentChar;
				}
			} else {
				if (isNumber(currentChar)) {
					editedValue = editedValue + currentChar;
				}
			}
		}
		sourceElement.value = editedValue;
	}
	function isNumber(character) {
		try {
			var number = Number(character);
			return number >= 0 && number <= 9;
		} catch (e) {
			return false;
		}
	}
	function getPrice(el) {
		try {
			el = parseInt($(el).attr('price'));
			if (isNaN(el)) {
				return 0;
			}
			return el;
		} catch (ex) {
			return 0;
		}
	}
	function getInt(el) {
		try {
			el = parseInt(el.value);
			if (isNaN(el)) {
				return 0;
			} 
			return el;
		} catch (ex) {
			return 0;
		}
	}
	function formatNumber(txt) {
		if (txt) {
			return (txt + '').replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1.");
		} else {
			return txt;
		}
	}
	function calculate(el) {
		var day = el.id.split('_')[1];
		var id = el.id.split('_')[2];
		var its = $('div[parent="order_' + day + '"]').find('input[id^="o_"]');
		var count = 0, countMin = 0, total = 0, totalMin = 0;
		for (var i=0; i < its.length; i+=3) {
			id = its[i].id.split('_')[2];
			var c = getInt(its[i]);
			var cm = getInt(its[i + 1]);
			var p = getPrice(its[i]);
			var pm = getPrice(its[i + 1]);
			count += c;
			countMin += cm;
			var to = c*p, tom = cm*pm;
			total += to;
			totalMin += tom;
			$('#o_'+day+'_'+id+'_total').text(formatNumber(to + tom));
		}
		$('#o_'+day+'_count').text(formatNumber(count));
		$('#o_'+day+'_count_min').text(formatNumber(countMin));
		$('#o_'+day+'_total').text(formatNumber(total + totalMin));
	}
	function doOrder(day) {
		var its = $('div[parent="order_' + day + '"]').find('input[id^="o_"]');
		var count = 0, countMin = 0, total = 0, totalMin = 0;
		var ids = '', quantity = '', miniQuantity = '', description = '';
		for (var i=0; i < its.length; i+=3) {
			id = its[i].id.split('_')[2];
			var c = getInt(its[i]);
			var cm = getInt(its[i + 1]);
			var des = its[i + 2].value;
			if (c != 0 || cm != 0) {
				ids += (i==0?'':'&') + 'ids=' + id;
				quantity += (i==0?'':'&') + 'quantity=' + c;
				miniQuantity += (i==0?'':'&') + 'miniQuantity=' + cm;
				description += (i==0?'':'&') + 'description=' + des;
			}
		}
		if (ids == '') {
			$('#o_'+day+'_messageError').html('Chưa nhập số lượng món');
			$('#o_'+day+'_messageInfor').html('');
			return;
		}
		var data = ids + '&' + quantity + '&' + miniQuantity + '&' + description + '&code=' + code + '&date=' + $('#date').text();
		$('#o_'+day+'_messageError').html('');
		$('#o_'+day+'_messageInfor').html('<img src="${loading}"/>');
		
		$.ajax({
		  type: "POST",
		  url: '${orderUrl}',
		  data: data,
		  success: function(data) {
		  	$('#o_'+day+'_messageInfor').html('');
			if (data.error != null) {
				$('#o_'+day+'_messageError').html(data.error);
			}
			if (data.success != null) {
				$('#o_'+day+'_messageInfor').html(data.success);
				resetCode();
				doReset(day);
			}
		  },
		  dataType: 'json',
		  contentType: "application/x-www-form-urlencoded; charset=UTF-8"
		});
	}
	function doReset(day) {
		var its = $('div[parent="order_' + day + '"]').find('input[id^="o_"]');
		for (var i=0; i < its.length; i++) {
			its[i].value='';
		}
		$('#o_'+day+'_count').text('0');
		$('#o_'+day+'_count_min').text('0');
		$('#o_'+day+'_total').text('0đ');
	}
</script>
]]>
